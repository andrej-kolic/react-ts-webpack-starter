{
  "name": "@andrej-kolic/react-ts-webpack-starter",
  "version": "0.3.0",
  "description": "",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrej-kolic/react-ts-webpack-starter.git"
  },
  "scripts": {
    "prepare": "husky install",
    "build": "webpack --config ./webpack/webpack.prod.js",
    "lint": "eslint . --cache",
    "lint:fix": "pnpm lint --fix --quiet",
    "format": "prettier --config .prettierrc . --write && pnpm lint:fix",
    "test": "pnpm test:type && pnpm test:code:coverage",
    "test:code": "NODE_ENV=test jest --runInBand",
    "test:code:coverage": "pnpm test:code --coverage",
    "test:type": "tsc --noEmit --pretty",
    "start": "webpack serve --config ./webpack/webpack.dev.js",
    "serve": "serve",
    "clean": "pnpm clean:cache && pnpm clean:dist",
    "clean:cache": "rimraf .eslintcache",
    "clean:dist": "rimraf ./dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/dotenv-safe": "^8.1.2",
    "@types/jest": "^29.2.6",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "css-loader": "^6.7.3",
    "dotenv-safe": "^8.2.0",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-testing-library": "^5.10.1",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.4.2",
    "mini-css-extract-plugin": "^2.7.2",
    "msw": "^1.0.1",
    "prettier": "^2.8.3",
    "pretty-quick": "^3.1.3",
    "rimraf": "^4.1.1",
    "serve": "^14.2.0",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "axios": "^1.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.8.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@testing-library/dom"
      ]
    }
  }
}
